<% provide(:title, "Registration") %>

<div class="row">
  <div class="col-md-4 col-md-offset-1">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Sign up using your personal Email</h3>
      </div>
      <div class="panel-body">
        <%= render "form" %>
      </div>
    </div>
    <%= render "devise/shared/links" %>
  </div>

  <div class="col-md-4 col-md-offset-1">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Sign up using your MENA Devs Slack login</h3>
      </div>
      <div class="panel-body">
        <%= render "devise/shared/omniauthable" %>
      </div>
    </div>
  </div>
</div>
