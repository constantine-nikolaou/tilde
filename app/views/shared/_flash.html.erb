<%# Notice %>
<% if flash[:notice].present? %>
  <div class="style-msg successmsg">
    <div class="sb-msg"><%= flash[:notice] %></div>
  </div>
<% end %>

<%# Alert %>
<% if flash[:alert].present? %>
  <div class="style-msg errormsg">
    <div class="sb-msg"><%= flash[:alert] %></div>
  </div>
<% end %>
