<% provide(:title, "Members") %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Member name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td>
          <div class="btn-group">
            <%= link_to user, :class => "btn btn-xs btn-default" do %>
              <%= fa_icon("eye", :text => "Show") %>
            <% end %>
          </div>
      </tr>
    <% end %>
  </tbody>
</table>
